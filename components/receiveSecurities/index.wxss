/* components/receiveSecurities/index.wxss */ 
.receiveCouponWrap {} 
 
.receiveCoupon { 
    width: 250rpx; 
    height: 98rpx; 
    display: inline-block; 
    background-repeat: no-repeat; 
    background-size: 100% 100%; 
    margin-right: 14rpx; 
} 
 
.receiveCouponFlex { 
    width: 100%; 
    height: 100%; 
    padding-left: 10rpx; 
    display: flex; 
    justify-content: space-between; 
    align-items: center; 
} 
 
.coupon_wrap { 
    padding-left: 6rpx; 
} 
 
.couponWrap>view:nth-child(1) { 
    height: 48rpx; 
    font-size: 48rpx; 
    line-height: 48rpx; 
    font-family: San-Francisco-Text-Bold; 
    font-weight: bold; 
    color: rgba(255, 51, 102, 1); 
} 
 
.couponWrap>view:nth-child(1)::before { 
    content: 'ï¿¥'; 
    height: 24rpx; 
    line-height: 24rpx; 
    font-size: 24rpx; 
    font-family: PingFang-SC-Bold; 
    font-weight: bold; 
    color: rgba(255, 51, 102, 1); 
} 
 
.couponWrap>view:nth-child(2) { 
    height: 24rpx; 
    font-size: 24rpx; 
    line-height: 24rpx; 
    font-family: San-Francisco-Text-Bold; 
    font-weight: bold; 
    color: #FF4F7B; 
    margin-top: 6rpx; 
} 
 
.couponWrap_>view:nth-child(1) { 
 
    color: #FEC3D4 !important; 
} 
 
.couponWrap_>view:nth-child(1)::before { 
    color: #FEC3D4 !important; 
} 
 
.couponWrap_>view:nth-child(2) { 
    color: #FEC3D4 !important; 
} 
 
.couponReceive { 
    height: 24rpx; 
    font-size: 24rpx; 
    font-family: PingFang-SC-Bold; 
    font-weight: bold; 
    color: rgba(255, 51, 102, 1); 
    line-height: 24rpx; 
    padding-right: 20rpx; 
} 
 
.HaveToReceive { 
    height: 24rpx; 
    font-size: 24rpx; 
    font-family: PingFang-SC-Bold; 
    font-weight: bold; 
    color: rgba(254, 195, 212, 1); 
    line-height: 24rpx; 
    padding-right: 20rpx; 
}