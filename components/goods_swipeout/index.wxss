.itemClass {
  padding: 0rpx;
}

.buttonRightItemClass {

  font-size: 28rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
}

.swipeoutWrap {
  border: none;
}

.container {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 304rpx;
  overflow: hidden;
  padding: 0rpx 24rpx;
  position: relative;
}

.container .left {
  width: 240rpx;
  height: 240rpx;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  position: relative;
  border-radius: 4rpx;
  overflow: hidden;
}

.container .left .left_themeIcon {
  position: absolute;
  top: 0;
  left: 0;
  max-width: 66rpx;
  max-height: 66rpx;
}

.container .left .left_img {
  width: 240rpx;
  height: 240rpx;
}

.container .left .left_tag {
  width: 86rpx;
  height: 27rpx;
  background: rgba(0, 0, 0, 1);
  opacity: 0.6;
  border-radius: 4px;
  line-height: 27rpx;
  position: absolute;
  left: 0;
  bottom: 0;
  text-align: center;
  font-size: 20rpx;
  color: rgba(243, 243, 243, 1);
  font-family: PingFangSC-Regular;
  font-weight: 400;
}

.container .right {
  flex: 1;
  display: flex;
  align-content: space-between;
  font-size: 20rpx;
  flex-wrap: wrap;
  position: relative;
  margin-left: 20rpx;
  height: 240rpx;
}

.container .right .right_top {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.container .right .right_top .right_top_active {
  flex: 1;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.container .right .right_top .right_top_active .right_top_tag1 {
  width: 52rpx;
}

.container .right .right_title {
  width: 392rpx;
  font-size: 28rpx;
  color: #2f3a40;
  margin-top: 7rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.container .right .right_msg {
  width: 100%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin-top: 9rpx;
  color: #889696;
  padding-right: 60rpx;
}

.right_msg text {
  font-size: 24rpx;
}

.goodList_msg_txt_two {
  margin-left: 40rpx;
}

.container .right .right_price {
  font-size: 32rpx;
  color: #f36;
  font-weight: bold;
  display: flex;
  justify-content: flex-start;
}

.container .right .right_price .right_price_ {
  position: relative;
  margin-left: 20rpx;
}

.container .right .right_price .right_price_::before {
  display: block;
  content: '¥';
  position: absolute;
  left: -8px;
  top: -6px;
  font-size: 24rpx;
  line-height: 36px;
  color: #f36;
  font-weight: normal;
}

.right_price_two {
  font-size: 20rpx;
  line-height: 24px;
  color: #f36;
  font-weight: normal;
  margin-left: 3rpx;
}

.container .right .right_bottom {
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
  width: 100%;
}

.goodlist_roa_price_wrap {
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.container .right .right_bottom .right_bottom_tag {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin-left: 12rpx;
}

.single {
  font-size: 18rpx;
  width: 30rpx;
}

.multiple {
  font-size: 18rpx;
  width: 30rpx;
  height: 24rpx;
}

.toBuy {
  width: 70rpx;
  line-height: 80rpx;
  text-align: center;
  z-index: 5;
  position: absolute;
  right: -20rpx;
  bottom: -15rpx;
}

.addCart_img {
  width: 30rpx;
  height: 30rpx;
  vertical-align: middle;
}

.top_active_tags {
  padding: 0rpx 8rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 4rpx;
  margin-right: 8rpx;
  color: #fff;
  height: 32rpx;
  line-height: 32rpx;
}

.top_active_tags image {
  width: 20rpx;
  height: 20rpx;
}

.top_active_tags text {
  font-size: 24rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
}

.right_bottom_tag {
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.right_bottom_tag view {
  font-size: 20rpx;
  color: #ff9600;
  height: 32rpx;
  line-height: 32rpx;
  padding: 0rpx 8rpx;
  border: 1rpx solid #ff9600;
  margin-right: 8rpx;
  border-radius: 4rpx;
}

.bottom_tag_discount {
  font-weight: bold;
}

.bottom_tag_savePrice {
  font-size: 22rpx;
  font-weight: 500;
}

.bottom_tag_savePrice::before {
  content: '省￥';
}

.top_active_tags_vip {
  height: 36rpx;
}

.top_active_tags_vip .viptag {
  width: 110rpx;
  height: 36rpx;
}

.noLook {
  background: #f3f3f3;
  color: #ccc;
  font-size: 28rpx;
}