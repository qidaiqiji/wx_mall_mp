<!--pages/accredit/accredit.wxml-->
<view wx:if="{{canIUse}}">
    <view class='header'>
        <title></title>
    </view>
 
    <view class='content'>
       <image class="img_photo" src="{{img_photo}}" ></image>
    </view>
 
    <button class='bottom' style="background:#ff3366" type='warn' open-type="getUserInfo" lang="zh_CN" bindgetuserinfo="bindGetUserInfo">
        点击进行微信授权
    </button>
</view>
 
<view wx:else>请升级微信版本</view>