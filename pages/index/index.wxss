  top: 4rpx;
}

.dot {
  position: absolute;
  top: 10rpx;
  right: 24rpx;
  width: 8rpx;
  height: 8rpx;
  border-radius: 50%;
  background: #fff;
}

.pro_swipe_top {
  width: 438rpx;
  height: 48rpx;
  border-radius: 24rpx;
  background: rgba(0, 0, 0, 0.5);
  overflow: hidden;
  opacity: 1;
  transform: translateY(0rpx);
  transition: all 0.6s linear;
}

