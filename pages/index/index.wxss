}

.Mask {
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  right: 0rpx;
  bottom: 0rpx;
  background: rgba(0, 0, 0, 0.7);
  z-index: 9991;
}
.message {
  width: 60rpx;
