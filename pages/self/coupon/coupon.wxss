page{
  height: 100%;
  background: #F3F3F3;
}

.title_img {
  width: 100%;
  height: 240rpx;

}

.title_img image {
  width: 100%;
}

.main {
  width: 100%;
  padding: 0 24rpx;
  overflow: hidden;
  background: #F3F3F3;
  flex: 1;
  position: relative;
  left: 0rpx;
  top: 0rpx;
}

.main_top {
  width: 750rpx;
  height: 98rpx;
  background: rgba(243, 243, 243, 1);
  position: absolute;
  left: 0rpx;
  top: 0rpx;
}
.changes{
  width: 750rpx;
  height: 98rpx; 
  background: rgba(243, 243, 243, 1);
}
.tabTitle {
  width: 100%;
  height: 98rpx;
  line-height: 98rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding: 0 24rpx;

}

.tabTitle_item {
  width: 110rpx;
  height: 48rpx;
  line-height: 48rpx;
  margin-right: 50rpx;
  font-size: 26rpx;
  color: #2F3A40;
  text-align: center;
  display: inline-block;
}

.on {
  background: #FF3366;
  border-radius: 24rpx;
  color: #fff;
}

.bottom_msg {
  position: fixed;
  left: 0rpx;
  bottom: 0rpx;
  z-index: 6;
  width: 100%;
  height: 152rpx;
  background: #F3F3F3;
}

.bottom_tip {
  width: 100%;
  height: 54rpx;
  line-height: 54rpx;
  color: #889696;
  font-size: 24rpx;
  text-align: center;
}


.my_coupon {
  width: 100%;
  height: 98rpx;
  line-height: 98rpx;
  text-align: center;

  color: #FF3366;
  font-size: 32rpx;
  background: #fff;
}

.not_data {
  width: 314rpx;
  height: 300rpx;
  position: relative;
  left: 50%;
  top: 40%;
  transform: translate(-50%, -50%);
}

.not_data image {
  width: 314rpx;
  height: 180rpx;
}

.not_data view {
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 26rpx;
  color: #B2B2B2;
}

.fixed {
  padding: 0rpx 24rpx;
  position: fixed;
  left: 0rpx;
  top: 0rpx;
  z-index: 8;
}