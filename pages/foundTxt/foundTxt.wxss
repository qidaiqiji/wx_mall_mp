/* pages/foundTxt/foundTxt.wxss */
@import "/utils/wxParse/wxParse.wxss";

page {
  background: #f3f3f3;
}

.wrap {}

.wrap_tap {
  width: 750rpx;
  padding-left: 24rpx;
  padding-right: 24rpx;
  padding-top: 20rpx;
}

.header {
  font-size: 48rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: rgba(47, 58, 64, 1);
  line-height: 64rpx;
  margin-top: 75rpx;
}

.conter {
  margin-top: 30rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(178, 178, 178, 1);
  line-height: 48rpx;
  display: flex;
  justify-content: flex-start;
}

.related_to_recommend {
  padding-left: 24rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: rgba(47, 58, 64, 1);
  padding-top: 46rpx;
  padding-bottom: 24rpx;
}

.conterRight {
  margin-left: 50rpx;
}

.bottom {
  padding-top: 28rpx;
  margin-bottom: 40rpx;
  padding-bottom: 32rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
  border-top: 1rpx solid #f3f3f3;
}

/* 卡片 */

.found_card {
  width: 750rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.brand_wrap {
  padding-top: 30rpx;
  margin: 0rpx 24rpx 0rpx 24rpx;
  display: flex;
  justify-content: space-between;
}

.brand_left {
  display: flex;
  justify-content: flex-start;
}

.brand_text {
  margin-left: 16rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.brand_time {
  font-size: 24rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(178, 178, 178, 1);
  line-height: 24rpx;
}

.card_wrap {
  width: 702rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 20rpx;
  margin-bottom: 20rpx;
}

.brand_tag {
  width: 70rpx;
  height: 70rpx;
  border: 1rpx solid rgba(220, 228, 230, 1);
  border-radius: 4rpx；;
}

.brand_tag>image {
  width: 100%;
  height: 100%;
  border-radius: 4rpx;
}

.brand_name {
  width: 426rpx;
  height: 34rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: rgba(47, 58, 64, 1);
  line-height: 34rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.tetx_wrap {
  margin-left: 23rpx;
  margin-top: 20rpx;
}

.tetx_describe_up {
  word-break: break-all;
  width: 650rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(47, 58, 64, 1);
  line-height: 46rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
}

.tetx_describe_down {
  word-break: break-all;
  width: 650rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(47, 58, 64, 1);
  line-height: 42rpx;
}

.fullText {
  font-size: 32rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(255, 88, 130, 1);
  line-height: 42rpx;
  margin-top: 15rpx;
}

.img_wrap {
  margin-left: 23rpx;
  margin-top: 39rpx;
}

.insets_img {
  width: 210rpx;
  max-height: 210rpx;
  border-radius: 4rpx;
  margin-right: 12rpx;
  margin-bottom: 12rpx;
}

.a_picture {
  max-width: 500rpx;
  max-height: 374rpx;
  overflow: hidden;
}

.first_img_width {
  overflow: hidden;
  border-radius: 4rpx;
  margin-right: 12rpx;
  margin-bottom: 12rpx;
}

.number_wrap {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 12rpx 24rpx 28rpx 26rpx;
}

.cart_wrap_two {
  width: 106rpx;
  height: 40rpx;
}

.cart_wrap {
  width: 106rpx;
  height: 40rpx;
  border-radius: 22rpx;
  display: flex;
  justify-content: center;
  line-height: 40rpx;
  background: rgba(255, 51, 102, 1);
  font-size: 22rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
}

.collection_wrap {
  display: flex;
  align-items: center;
}

.collection_wrap>text {
  padding-left: 2rpx;
  font-size: 22rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(178, 178, 178, 1);
}

.collection_wrap>image {
  width: 30rpx;
  height: 30rpx;
}

.cart_img {
  width: 30rpx;
  height: 30rpx;
  margin-left: 12rpx;
}

.video_wrap {
  width: 654rpx;
  height: 368rpx;
  margin-left: 24rpx;
  margin-top: 30rpx;
}

.reading_txt {
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  font-weight: 500;
  color: rgba(178, 178, 178, 1);
}

.found_bottom_txt {
  height: 181prx;
  font-size: 26rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(212, 212, 212, 1);
  line-height: 181rpx;
  text-align: center;
}

.found_bottom {
  width: 100%;
  height: 378rpx;
  background: #f3f3f3;
}

.The_horn {
  width: 654rpx;
  height: 78rpx;
  background: rgba(243, 243, 243, 1);
  border-radius: 4rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 12rpx;
  margin-top: 10rpx;
  margin-left: 24rpx;
  padding: 0rpx 31rpx;
}

.the_horn_left {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.horn_img {
  width: 30rpx;
  height: 30rpx;
}

.horn_txt {
  font-size: 26rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(0, 135, 234, 1);
  margin-left: 8rpx;
  line-height: 42rpx;
}

.myVideo {
  width: 100%;
  height: 368rpx;
}

.imgcover {
  width: 100%;
  height: 368rpx;
}

.video-host-wrapper {
  width: 100%;
  height: 368rpx;
  position: relative;
}

.showVideo {
  display: flex;
  align-items: center;
  width: 100%;
  height: 368rpx;
  background: #000;
}

.playbtn {
  position: absolute;
  width: 120rpx;
  height: 120rpx;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}

.container view {
  width: 100%;
}

.all_text {
  font-size: 32rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(47, 58, 64, 1);
  line-height: 46rpx;
  padding-right: 54rpx;
}

.scrollView {
  height: 92rpx;
}

.found_up_new {
  width: 750rpx;
  height: 70rpx;
  background: rgba(255, 226, 233, 1);
  font-size: 28rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(255, 51, 102, 1);
  line-height: 70rpx;
  text-align: center;
}

.collection_wrap_two {
  width: 110rpx;
}