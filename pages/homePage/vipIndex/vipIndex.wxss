/* pages/test/test.wxss */
page {
  background: #f3f3f3;

}
.all{
  width:100%;
  overflow: hidden;
  }
.vip_header{
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  background-size: 100%;
  background-repeat: no-repeat;
  width: 100%;
}

.vip_header_nav {
  display: flex;
  width: 100%;
  height: 208rpx;
  padding: 0 24rpx;
}

.header_nav_jump {
  width: 100%;
  display: flex;
  justify-content: space-around;
  align-items: center;
}

.jump_item {
  width: 120rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;

}

button {
  height: 110rpx;
}
button .jump_item .jump_item_title{
  margin: 0;
}

button::after {
  border: none;
}

.jump_item_img {
  width: 60rpx;
  height: 60rpx;
}

.jump_item_title {
  margin-top: 13rpx;
  font-size: 24rpx;
  text-align: center;
  color: #FAE8BE;
}

.header_nav_slideshow {
  width: 750rpx;
  height: 336rpx;
  border-radius: 20rpx;
  overflow: hidden;
}

.slide-image {
  width: 702rpx;
  height: 336rpx;
  border-radius: 20rpx;
  margin-left: 24rpx;
}

.vip_ad {
  width: 100%;
  margin-bottom: 54rpx;
}

.vip_ad_title {
  width: 100%;
  height: 88rpx;

}

.vip_ad_title image {
  width: 100%;
  height: 88rpx;
}

.vip_ad_content {
  width: 100%;
  height: 466rpx;
  padding: 0 22rpx 0 24rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  align-content: center;
}

.vip_ad_content view {
  margin-bottom: 14rpx;
}

.big_ad_img {
  width: 464rpx;
  height: 226rpx;
  background: #fff;
}

.big_ad_img_ {
  width: 464rpx;
  height: 226rpx;
}

.ad_img {
  width: 226rpx;
  height: 226rpx;
  background: #fff;
}

.ad_img_ {
  width: 226rpx;
  height: 226rpx;
}

.mxdp {
  width: 100%;
  background-size: 100%;
  background-repeat: no-repeat;
  padding-top: 32rpx;
  margin-bottom: 6rpx;
}

.mdbb {
  width: 100%;

  background-size: 100%;
  background-repeat: no-repeat;
  padding-top: 32rpx;
  margin-bottom: 6rpx;
}

.mxdp_title,
.mdbb_title {
  font-size: 32rpx;
  font-weight: bold;
  color: rgba(255, 255, 255, 1);

  padding: 0 24rpx;
}

.mxdp_msg,
.mdbb_msg {
  font-size: 26rpx;
  color: rgba(255, 255, 255, 1);
  margin-bottom: 40rpx;
  padding: 0 24rpx;
}

.mxdp_list,
.mdbb_list {
  width: 100%;
  padding: 0 24rpx;
}

.vip_goodsList {
  width: 100%;

}

.vip_goodsList_title {
  width: 100%;
  height: 88rpx;
}

.vip_goodsList_title image {
  width: 100%;
  height: 88rpx;
}

.vip_goodsList_content {
  width: 100%;
  background: #fff;
}