.i-sticky-item-header {
  background: #eee;
  font-size: 14px;
  width: 100%;
  height: 32px;
  line-height: 32px;
  z-index: 800;
}

.i-sticky-item-content {
  font-size: 14px;
  padding-top: 20rpx;
}

.i-sticky-title {
  width: 100%;
  box-sizing: border-box;

  
  background: #eee;
}

.i-sticky-fixed .i-sticky-title {
  position: fixed;
  top: 0;
  z-index: 800;
}
